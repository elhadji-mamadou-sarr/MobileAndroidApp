<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/titre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/isep_marron_foncÃ©e"
        android:textSize="@dimen/dim30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/debut_promo"
        android:layout_marginTop="@dimen/dim50"
        android:textColor="@color/isep_marron"
        app:layout_constraintTop_toBottomOf="@+id/titre"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="5dp" />

    <TextView

        android:id="@+id/id_textViewDebut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=": 2022"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <TextView
        android:id="@+id/finTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fin_promo"
        android:textColor="@color/isep_marron"
        android:layout_marginTop="@dimen/dim10"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/anneeTextViewFin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=": 2023"
        app:layout_constraintBottom_toBottomOf="@+id/finTextView"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

</androidx.constraintlayout.widget.ConstraintLayout>